{
    "compilerOptions": {
        "target": "es6",
        "allowJs": true,
        "jsx": "react",
        "outDir": "./dist",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "watch": true,
        "skipLibCheck": true,
        "moduleResolution":"node"
    },
    "include": [
        // "typings/**/*.d.ts",
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/*.tsx",
        "src/*.ts"

    ],
    "exclude": [
        "node_modules"
    ], 
}
